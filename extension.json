{
	"name": "Page Schemas",
	"version": "0.5",
	"author": [
		"Yaron Koren",
		"Ankit Garg",
		"..."
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Page_Schemas",
	"descriptionmsg": "ps-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "specialpage",
	"GroupPermissions": {
